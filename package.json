{
  "name": "fhevm-universal-sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building confidential dApps with FHEVM - Framework agnostic, developer friendly",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:sdk && npm run install:example",
    "install:sdk": "cd packages/fhevm-sdk && npm install",
    "install:example": "cd examples/renewable-energy-market && npm install",
    "build:sdk": "cd packages/fhevm-sdk && npm run build",
    "test:sdk": "cd packages/fhevm-sdk && npm test",
    "test:example": "cd examples/renewable-energy-market && npm test",
    "test:all": "npm run test:sdk && npm run test:example",
    "deploy:example": "cd examples/renewable-energy-market && npm run deploy",
    "verify:example": "cd examples/renewable-energy-market && npm run verify",
    "interact:example": "cd examples/renewable-energy-market && npm run interact",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules",
    "clean:build": "rm -rf packages/*/dist examples/*/artifacts examples/*/cache"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "homomorphic-encryption",
    "zama",
    "ethereum",
    "privacy",
    "confidential",
    "blockchain",
    "sdk",
    "react",
    "vue",
    "nextjs",
    "universal"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zama-ai/fhevm-react-template"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
